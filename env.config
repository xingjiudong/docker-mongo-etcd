# Docker volume driver
export VOLUME_DRIVER=rbd

# Etcd Endpoints IP
export ETCD_CLIENT_IP=

# For docker swarm cluster
export DOCKER_SWARM_URI=
export COMPOSE_OPT=

# Versions
export MONGO_VERSION=latest
export HAPROXY_VERSION=latest

# Mongodb Information
export MONGO_INITDB_ROOT_USERNAME=siteRootAdmin
export MONGO_INITDB_ROOT_PASSWORD=password
export REPLSET_NAME=
export DNS_SERVER=

# Haproxy Information
export HAPROXY_NODE=
export PROJECT_NAME=
export TIMEOUT_CONNECT=5s
export TIMEOUT_CLIENT=50s
export TIMEOUT_SERVER=50s
export TIMEOUT_HTTP_KEEP_ALIVE=50s
export MAXCONN=2000
